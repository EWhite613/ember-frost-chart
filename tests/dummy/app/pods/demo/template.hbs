{{#frost-chart
  class='demo-full'
  hook='demo-power'
  xDomain=xDomain
  yDomain=yDomain
  yScale=(d3-linear-scale)
  as |chart|
}}
  {{#chart.x-axis
    label='Foo'
    as |tick|
  }}
    {{to-fixed tick 0}}
  {{/chart.x-axis}}

  {{#chart.y-axis
    label='Bar'
    as |tick|
  }}
    {{to-fixed tick 0}}
  {{/chart.y-axis}}

  {{#chart.svg as |plot|}}
    {{plot.grid}}

    {{!-- {{#plot.bar data=data x='x' y='y' as |bar|}}
      <rect x={{bar.x}} y={{bar.y}} width={{bar.width}} height={{bar.height}} />
    {{/plot.bar}} --}}

    {{!plot.line data=data x='x' y='y'}}

    {{#plot.scatter data=data x='x' y='y' as |point|}}
      <circle cx={{point.x}} cy={{point.y}} r=4 fill='red' />
    {{/plot.scatter}}
  {{/chart.svg}}
{{/frost-chart}}


